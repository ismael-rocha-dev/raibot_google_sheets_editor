{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "import/extensions": ["error", "always"],
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-restricted-syntax": "off"
  }
}
